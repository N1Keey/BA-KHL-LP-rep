{% extends "_base.j2" %}
{% set active_link = 'Suchseite' %}
{% block styles %}
  {{ super() }}
  <style>
  </style>
{% endblock %}
{% block title %} <title>Suchseite</title> {% endblock %}
{% block navbar %}
  {{ super() }}
{% endblock %}
{% block left_navbar %}
    <div class='left_navbar'>

    </div>
{% endblock %}
{% block content %}
    <div class='content'>
        <h3>{{ foundkrankheitendict.get('Foundelement') }}</h3>
        <p>
            <b>Krankheit/en:</b>
            {% if foundkrankheitendict.get('Hideouts') != [] %}
                {# Dict=> 'Hideouts' : {'Krankheit':krankheit, 'Umstand':umstand} #}
                {% for krankheit in foundkrankheitendict.get('Hideouts') %}
                    <u>{{ krankheit.get('Krankheit') }} ({{ krankheit.get('Umstand') }})</u>
                {% endfor %}
            {% else %}
                {# Element ist so nicht oder garnicht in Datenbank enthalten #}
                <u>Keine Krankheit gefunden</u>
            {% endif %}
        </p>
    </div>
{% endblock %}